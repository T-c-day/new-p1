  </servlet>
  <servlet>
  	<servlet-name>SignUpController</servlet-name>
  	<display-name>SignUpController</display-name>
  	<description></description>
  	<servlet-class>com.training.controller.SignUpController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeePendingServlet</servlet-name>
  	<display-name>EmployeePendingServlet</display-name>
  	<description></description>
  	<servlet-class>com.training.controller.EmployeePendingServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeResolvedServlet</servlet-name>
  	<display-name>EmployeeResolvedServlet</display-name>
  	<description></description>
  	<servlet-class>com.training.controller.EmployeeResolvedServlet</servlet-class>
  </servlet>
