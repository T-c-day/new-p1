  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SubmitReimbursementController</servlet-name>
  	<url-pattern>/submitrb</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateEmployeeController</servlet-name>
  	<url-pattern>/UpdateEmployeeController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateEmployeeController</servlet-name>
  	<url-pattern>/updateemployee</url-pattern>
  </servlet-mapping>
