  </servlet>
  <servlet>
  	<servlet-name>UpdateEmployeeController</servlet-name>
  	<display-name>UpdateEmployeeController</display-name>
  	<description></description>
  	<servlet-class>com.training.controller.UpdateEmployeeController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ManageReimbursementController</servlet-name>
  	<display-name>ManageReimbursementController</display-name>
  	<description></description>
  	<servlet-class>com.training.controller.ManageReimbursementController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SignUpController</servlet-name>
  	<url-pattern>/SignUpController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SignUpController</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeePendingServlet</servlet-name>
  	<url-pattern>/EmployeePendingServlet</url-pattern>
